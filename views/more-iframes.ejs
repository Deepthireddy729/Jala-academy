<%- include('layout', { 
  title: 'More - iFrames', 
  body: `
    <section class="dashboard">
      <h1>iFrames</h1>
      <div class="tabs">
        <div class="tabs-header">
          <button class="tab-btn active" data-tab="frame1">Iframes</button>
        </div>
        <div class="tabs-body">
          <div class="tab-panel active" id="frame1">
            <div style="margin-top:1rem;">
              <iframe id="frameContent" src="/iframe/frame-one" title="Frame One" style="width:100%; height:500px; border:1px solid #e5e7eb; border-radius:0.5rem;"></iframe>
            </div>
            <div style="margin-top:1rem; display:flex; gap:0.75rem; justify-content:center;">
              <button class="btn" onclick="loadFrame('/iframe/frame-one')">Frame One</button>
              <button class="btn" onclick="loadFrame('/iframe/frame-two')">Frame Two</button>
              <button class="btn" onclick="loadFrame('/iframe/frame-three')">Frame Three</button>
            </div>
          </div>
        </div>
      </div>

      <script>
        function loadFrame(url) {
          document.getElementById('frameContent').src = url;
        }
      </script>
    </section>
  ` 
}) %>


